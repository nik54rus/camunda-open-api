{
    "name": "nik54rus/camunda-open-api",
    "description": "",
    "keywords": [
        "camunda",
        "Rest API",
        "v3.0",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "nik54rus",
            "homepage": "https://github.com/nik54rus/camunda-open-api"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Camunda\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Camunda\\Client\\" : "test/" }
    }
}
